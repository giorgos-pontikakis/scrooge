      (labels ((create-accounts (account-list parent-id)
                 (let (list)
                   (mapc (lambda (acc)
                           (let* ((db-pid parent-id)
                                  (inst (make-instance 'account
                                                       :title (second acc)
                                                       :parent-id db-pid
                                                       :debit-p *debitp*)))
                             (insert-dao inst)
                             (push inst list)
                             (create-accounts (third acc)
                                              (id (select-dao-unique 'account (:= 'title
                                                                                  (second acc)))))))
                         account-list)
                   list)))
        (setf *debitp* nil)
        (create-accounts credit-accounts :null)
        (setf *debitp* t)
        (create-accounts debit-accounts :null)))))


